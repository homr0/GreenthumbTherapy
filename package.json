    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
